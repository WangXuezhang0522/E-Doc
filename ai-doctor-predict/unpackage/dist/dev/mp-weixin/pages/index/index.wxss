
	/* 更改vant组件css样式 */
page {
		--notice-bar-height: 16rpx;
		--notice-bar-padding: 32rpx;
		--notice-bar-font-size: 26rpx;
}
	
	/* 全局设置 */
.overall {
		position: relative;
		background-color: #f8f8f8;
		height: 3100rpx;
		width: 100%;
	    animation: overall 2s;
}
@keyframes overall{
from{
			opacity: 0;
}
to{
			opacity: 1;
}
}
/* 当用户未登录时禁止用户进行操作 */
.banAction{
	   top: 0;
	   left: 0;
	   background-color: rgba(0, 255, 127, 0.0);
       position: absolute;
	   width: 100%;
	   height: 100%;
}
	/* 搜索框 */
.search {
		height: 64rpx;
		width: 90%;
		display: flex;
		margin: 0px auto;
}
.search .font image {
		margin-top: 6rpx;
		width: 32rpx;
		height: 32rpx;
}
.search input {
		font-size: 28rpx;
		margin-left: 12rpx;
}
.search .scan{
		width: 38rpx;
		height: 38rpx;
		margin-left: 280rpx;
		margin-top: 5rpx;
}
	/* 遮罩层样式设置 */
.wrapper {
		display: none;
		position: fixed;
		top: 200rpx;
		left: 0;
		z-index: 98;
		display: flex;
		align-items: center;
		justify-content: center;
		height: 2600rpx;
}
.block {
		width: 730rpx;
		height: 2900rpx;
		transition: all 2s linear;
}
@keyframes blockAm {
from {
			background-color: rgba(22, 22, 22, 0.4);
}
to {
			background-color: rgba(22, 22, 22, 0.9);
}
}


	/* Ai问诊区域 */
.ai {
		display: flex;
		justify-content: space-between;
		height: 236rpx;
		width: 708rpx;
		margin: 20rpx auto;
}

	/* 设置盒子 */
.ai .Interrogation .box,
	.ai .psychology .box {
		display: flex;
		margin-left: 20rpx;
		margin-top: 32rpx;
}

	/* ai问诊*/
.ai .Interrogation {
		width: 340rpx;
		/* background-color: aqua; */
		/* rgba(0, 255, 127, 0.1) */
		background-image: linear-gradient(to bottom, rgba(0, 255, 127, 0.1), rgba(255, 255, 255, 1));
		border-radius: 16rpx;
}
.ai .Interrogation .h1,
	.ai .psychology .h1 {
		font-size: 48rpx;
		font-weight: 600;
		letter-spacing: 4rpx
}
.ai .Interrogation .p,
	.ai .psychology .p {
		font-size: 24rpx;
		color: #8c8c8c;
		margin-top: 6rpx;
		margin-bottom: 8rpx;
}
.ai .Interrogation .right .img {
		height: 108rpx;
		width: 120rpx;
		margin-left: 16rpx;
		margin-top: 20rpx;
}

	/* ai心理 */
.ai .psychology {
		width: 340rpx;
		/* background-color: aqua; */
		background-image: linear-gradient(to bottom, rgba(255, 0, 0, 0.1), rgba(255, 255, 255, 1));
		border-radius: 16rpx;
}
.ai .psychology .right .img {
		height: 152rpx;
		width: 140rpx;
}

	/* 轮播图 */
.banner,
	.banner image {
		height: 320rpx;
		width: 700rpx;
		margin: auto;
		border-radius: 16rpx;
		background-size: contain;
}

	/* 健康+ */
.health {
		width: 700rpx;
		height: 2120rpx;
		margin: 30rpx auto;
		/* background-color: #ffffff; */
		border-radius: 12rpx;
}

	/* 头部区域 */
.health .top {
		font-size: 32rpx;
		font-weight: 550;
		margin-left: 12rpx;
		margin-right: 10px;
}
.health .ul {
		display: flex;
		height: 600rpx;
		flex-wrap: wrap;
		margin-top: -4rpx;
		width: 100%;
}
.health .ul .li {
		display: flex;
		width: 340rpx;
		height: 180rpx;
		margin-top: 16rpx;
		background-image: linear-gradient(to bottom, rgba(0, 255, 255, 0.1), rgba(255, 255, 255, 1));
		border-radius: 12rpx;
}
.health .ul .li:nth-child(2n) {
		margin-left: 20rpx;
}
.health .ul .li .h1 {
		font-size: 28rpx;
		width: 100%;
		font-weight: 550;
		margin-top: 12rpx;
		margin-left: 12rpx;
}
.health .ul .li .number {
		font-weight: 560;
		margin-top: 12rpx;
		margin-left: 12rpx;
		width: 200rpx;
}
.health .ul .li .p {
		font-size: 24rpx;
		margin-left: 12rpx;
		margin-top: 12rpx;
		color: #888888;
}
.health .ul .li .right {
		width: 200rpx;
}
.health .ul .li .left {
		display: flex;
		flex-wrap: wrap;
		width: 240rpx;
		height: 80rpx;
}
.health .ul .li .right image {
		height: 120rpx;
		width: 120rpx;
		margin-left: 18rpx;
}

