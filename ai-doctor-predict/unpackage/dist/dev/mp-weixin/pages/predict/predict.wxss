
.content {
		position: relative;
		width: 100%;
		height: 100%;
		font-family: 宋体;
		font-size: 32rpx;
}
.conversation {
		width: 100%;
		margin-top: 0vmax;
		height: 87vmax;
		border-radius: 15rpx;
		background-image: linear-gradient(to bottom, rgba(0, 255, 127, 0.1), rgba(255, 255, 255, 1));
		overflow-y: scroll
}
.user-message {
		margin-left: 128rpx;
		display: flex;
		min-height: 80rpx;
		min-width: 20%;
		margin-right: 24rpx;
		color: rgba(72, 72, 72, 0.9);
		justify-content: flex-end;
}
.img {
		width: 60rpx;
		height: 60rpx;
}
.usertext {
		padding: 16rpx;
		border-radius: 20rpx;
		font-family: STXinwei;
		font-size: 32rpx;
		font-weight: 400;
		letter-spacing: 0.8rpx;
		background-color: rgba(0, 255, 127, 0.1);
}
.robot-message {
		display: flex;
		justify-content: flex-start;
		margin-top: 32rpx;
		margin-left: 12rpx;
}
.robottext {
		background-color: #ffffff;
		width: 70%;
		margin-left: 6rpx;
		margin-top: 4rpx;
		font-family: STXinwei;
		font-size: 32rpx;
		padding: 16rpx;
		border-radius: 20rpx;
}
.formbox {
		/* margin-top: 10rpx; */
		height: 160rpx;
		width: 100%;
		background-color: #ffffff;
		border-radius: 10rpx;
}

	/* 边框 */
.formbox .box {
		display: flex;
		align-items: center;

		height: 90rpx;
		width: 90%;
		margin: 0px auto;
		border-radius: 40rpx;
		border: 6rpx solid rgba(172, 257, 231);
}
#text {
		font-size: 36rpx;
		font-family: STXinwei;
		width: 80%;
		height: 60rpx;
		padding: 20rpx;
}
#btn {
		display: block;
		border-radius: 36rpx;
		font-family: STXinwei;
		width: 170rpx;
		height: 80rpx;
		/* 		background: url('../../static/请柬.png') center/100% no-repeat; */
		background-color: rgba(123, 227, 187);
		line-height: 2.2;
		margin-right: 20rpx;
}
.vant-heart {
		margin-left: 70rpx;
}

	/* 	语音识别按钮 */
.voice {
		width: 46rpx;
		height: 64rpx;
		margin-bottom: 8rpx;
		margin-left: 8rpx;
		background-image: url('data:image/png;base64,');
		background-repeat: no-repeat;
		background-size: cover;
		border-style: none;
		background-color: rgba(255, 0, 0, 0.0);
		border: 0.2rpx solid rgba(255, 0, 0, 0.0);
}

	/* 搜索加载 */
.searchLoad {
		position: absolute;
		z-index: 99;
		top: 30%;
		left: 36%;
}
